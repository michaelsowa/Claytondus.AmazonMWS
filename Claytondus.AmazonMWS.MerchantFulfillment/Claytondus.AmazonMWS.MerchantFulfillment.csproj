    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Company>Amazon and Clayton Davis and Webthinking</Company>
    <Product>Webthinking.AmazonMWS.MerchantFulfillment</Product>
